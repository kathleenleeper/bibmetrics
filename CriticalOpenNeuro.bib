  author = {Johansen, P{\aa}l-{\O}rjan and Krebs, Teri Suzanne},
  month = mar,
  year = {2015},
  keywords = {epidemiology,hallucinogen,Psychedelic,PUBLIC health,suicide},
  pages = {270--279},
  file = {Johansen-Krebs-2015-Psychedelics_not_linked_to_mental_health_problems.pdf:/home/feline/Documents/div3/resources/Johansen-Krebs-2015-Psychedelics_not_linked_to_mental_health_problems.pdf:application/pdf;Snapshot:/home/feline/Documents/Zotero Files/storage/88A87KHT/270.html:text/html}
