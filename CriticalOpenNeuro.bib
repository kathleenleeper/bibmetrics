  month = nov,
  year = {2006},
  keywords = {Alternative Splicing,Animals,Biological Transport,Brain,Humans,Multigene Family,Receptors, Serotonin,serotonin,Signal Transduction,Synapses},
  pages = {553--572},
  file = {Bockaert_et-al-2006-Neuronal_5-HT_metabotropic_receptors.pdf:/home/feline/Documents/div3/resources/Bockaert_et-al-2006-Neuronal_5-HT_metabotropic_receptors.pdf:application/pdf}
