  number = {3},
  urldate = {2015-03-26},
  journal = {{PLoS} {ONE}},
  author = {Liu, Yutao and Garrett, Melanie E. and Dennis, Michelle F. and Green, Kimberly T. and Ashley-Koch, Allison E. and Hauser, Michael A. and Beckham, Jean C. and Kimbrel, Nathan A. and {VA Mid-Atlantic MIRECC Registry Workgroup}},
  month = mar,
