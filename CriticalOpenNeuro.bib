  year = {2012},
  pages = {e1002473},
  file = {Shan_et-al-2012-Ligand-Dependent_Conformations_and_Dynamics_of.pdf:/home/feline/Documents/div3/resources/Shan_et-al-2012-Ligand-Dependent_Conformations_and_Dynamics_of.pdf:application/pdf}
