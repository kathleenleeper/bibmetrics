  author = {Aghajanian, George Kevork and Marek, Gerard},
  month = aug,
  year = {1999},
  keywords = {5-HT2A receptors,Cerebral cortex,Excitatory postsynaptic potentials,Glutamatergic,hallucinogens,Locus coeruleus,LSD,serotonin},
  pages = {16S--23S},
  file = {Aghajanian-Marek-1999-Serotonin_and_Hallucinogens.pdf:/home/feline/Documents/div3/resources/Aghajanian-Marek-1999-Serotonin_and_Hallucinogens.pdf:application/pdf;Snapshot:/home/feline/Documents/Zotero Files/storage/V8KHHNWV/1395318a.html:text/html}
