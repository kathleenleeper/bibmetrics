  pages = {373--386},
  file = {Rajagopal_et-al-2010-Teaching_old_receptors_new_tricks.pdf:/home/feline/Documents/div3/resources/Rajagopal_et-al-2010-Teaching_old_receptors_new_tricks.pdf:application/pdf;Snapshot:/home/feline/Documents/Zotero Files/storage/NIKPXN7H/nrd3024.html:text/html}
}

@article{Ritter2009,
