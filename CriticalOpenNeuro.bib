  journal = {Science},
  author = {Wang, Chong and Jiang, Yi and Ma, Jinming and Wu, Huixian and Wacker, Daniel and Katritch, Vsevolod and Han, Gye Won and Liu, Wei and Huang, Xi-Ping and Vardy, Eyal and {McCorvy}, John D. and Gao, Xiang and Zhou, X. Edward and Melcher, Karsten and Zhang, Chenghai and Bai, Fang and Yang, Huaiyu and Yang, Linlin and Jiang, Hualiang and Roth, Bryan L. and Cherezov, Vadim and Stevens, Raymond C. and Xu, H. Eric},
  month = may,
  year = {2013},
  pages = {610--614},
  file = {Snapshot:/home/feline/Documents/Zotero Files/storage/W5CRFTEF/610.html:text/html}
