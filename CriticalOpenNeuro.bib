  month = jun,
  year = {2007},
  keywords = {gene gun,hallucinogen,in vitro electrophysiology,organotypic slices,serotonin},
  pages = {9870--9875},
  file = {BÃ©Ã¯que_et-al-2007-Mechanism_of_the_5-hydroxytryptamine_2A.pdf:/home/feline/Documents/div3/resources/BÃ©Ã¯que_et-al-2007-Mechanism_of_the_5-hydroxytryptamine_2A.pdf:application/pdf;Snapshot:/home/feline/Documents/Zotero Files/storage/NRD3XWCT/9870.html:text/html}
