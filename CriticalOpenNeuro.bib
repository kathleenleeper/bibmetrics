  month = oct,
  year = {1992},
  keywords = {(\ensuremath{\perp})-8-Hydroxy-2-(di-n-propylamino)tetralin HBr (8-OH-DPAT),5-HT1A receptors (human),5-HT2 receptors (human),G-protein coupling,Ketanserin},
  pages = {153--162},
  file = {ScienceDirect Snapshot:/home/feline/Documents/Zotero Files/storage/93NSFGVJ/092241069290123D.html:text/html;Stam et al_1992_Genomic organization, coding sequence and functional expression of human 5-HT2.pdf:/home/feline/Documents/div3/resources/Stam et al_1992_Genomic organization, coding sequence and functional expression of human 5-HT2.pdf:application/pdf}
