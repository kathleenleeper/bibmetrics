  month = sep,
  year = {1994},
  keywords = {Education (general)},
  pages = {513--547},
  file = {Snapshot:/home/feline/Documents/Zotero Files/storage/9BRFN633/BF02497086.html:text/html;Strenta_et-al-1994-Choosing_and_leaving_science_in_highly_selective.pdf:/home/feline/Documents/div3/resources/Strenta_et-al-1994-Choosing_and_leaving_science_in_highly_selective.pdf:application/pdf}
