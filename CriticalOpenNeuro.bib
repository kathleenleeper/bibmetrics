  number = {6132},
  urldate = {2015-04-10},
  journal = {Science},
  author = {Wacker, Daniel and Wang, Chong and Katritch, Vsevolod and Han, Gye Won and Huang, Xi-Ping and Vardy, Eyal and {McCorvy}, John D. and Jiang, Yi and Chu, Meihua and Siu, Fai Yiu and Liu, Wei and Xu, H. Eric and Cherezov, Vadim and Roth, Bryan L. and Stevens, Raymond C.},
  month = may,
