@misc{GraemeHirst2013a,
abstract = {{\textless}p{\textgreater}A term strangely missing from the Force11 Manifesto is {\textless}em{\textgreater}usability{\textless}/em{\textgreater}. There is little point in designing new tools or new systems of scholarly communication if they are too difficult to use; unless they are imposed by {\textless}em{\textgreater}coercion {\textless}/em{\textgreater}(see below), they will simply be ignored or abandoned after brief and frustrating use. There will be no {\textless}em{\textgreater}community acceptance{\textless}/em{\textgreater}. The present Manifesto contains no consideration of how the system that it wishes to overthrow evolved, what it is optimized for, and what specific aspects need to be maintained. For example, language (see below) works extremely well for communicating certain kinds of {\&}ldquo;knowledge objects{\&}rdquo;, such as argumentation, just as it fails for others, such as datasets. More generally, none of the Manifesto{\&}rsquo;s argument is actually couched in terms of how people actually communicate knowledge. The Manifesto also contains no consideration of human usability of any of the ideas that it proposes. Many of the proposals look wonderful in theory, but unless they are conceived right from the start in terms of how they could be adopted, transitioned to, and used, then they are non-starters. Or starters that have a too-high risk of subsequent failure. Again, none of the argument is actually couched in terms of how people actually communicate ideas. I will briefly discuss four facets of this issue.{\textless}/p{\textgreater}
},
author = {{Graeme Hirst}},
booktitle = {Beyond the PDF2},
keywords = {Making It Happen},
language = {eng},
title = {{Usability — a neglected dimension}},
url = {http://www.force11.org/Making It Happen},
year = {2013}
}
@misc{GraemeHirst2013,
abstract = {{\textless}p{\textgreater}Problem: Due to authors{\&}{\#}39; ignorance of past work, new research replicates, or fails to adequately take into account, previously published research.  Reasons: (1) Inaccessibility of legacy literature.  (2) Laziness, poor literature search, not-on-Google effect, false belief of irrelevance of all but the most recent research.  Solution to (2): Precedent-finding system takes the text of an author{\&}{\#}39;s early draft (or a submitted manuscript) and uses NLP-based text-similarity metrics that take different terminologies, synonymy, paraphrase, semantic roles, structure of argumentation, and citations into account to find potentially related ideas in published work.  NB: This is neither conventional search nor plagiarism detection.{\textless}/p{\textgreater}
},
author = {{Graeme Hirst}},
booktitle = {Beyond the PDF2},
keywords = {Visions for the Future},
language = {eng},
title = {{NLP methods for finding precedent research in the legacy literature}},
url = {http://www.force11.org/Visions},
year = {2013}
}
@techreport{Grigorov2015a,
abstract = {WHY Open Science in Horizon 2020? Open Science (OS) offers researchers tools and workflows for transparency, reproducibility, dissemination and transfer of new knowledge. Ultimately, this can also have an impact on in research evaluation exercises, e.g. Research Excellence Framework (REF), set to demand greater “societal impact” in future, rather than just research output1. OS can also be an effective tool for research managers to transfer knowledge to society, and optimize the use and re-use by unforeseen collaborators. For funders, OS offers a better return on investment (ROI) for public funding, and underpins the EU Digital Agenda by measurably contributing to economic growth. This brief showcases why and how Open Science can optimize your Horizon 2020 proposal evaluation.},
