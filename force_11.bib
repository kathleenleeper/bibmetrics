keywords = {Animals,Biomedical Research,Database Management Systems,Humans,Information Dissemination,Information Storage and Retrieval,Internet,Knowledge Bases,Medicine,Natural Language Processing,Neurodegenerative Diseases,Semantics,Vocabulary, Controlled},
language = {eng},
number = {5},
pages = {739--51},
title = {{The SWAN biomedical discourse ontology.}},
volume = {41},
year = {2008}
}
