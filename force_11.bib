@article{CameronNeylon2009c,
abstract = {{\textless}p{\textgreater}The means we use to record the process of carrying out research remains tied to the concept of a paginated paper notebook despite the advances over the past decade in web based communication and publication tools. The development of these tools offers an opportunity to re-imagine what the laboratory record would look like if it were re-built in a web-native form. In this paper I describe a distributed approach to the laboratory record based which uses the most appropriate tool available to house and publish each specific object created during the research process, whether they be a physical sample, a digital data object, or the record of how one was created from another. I propose that the web-native laboratory record would act as a feed of relationships between these items. This approach can be seen as complementary to, rather than competitive with, integrative approaches that aim to aggregate relevant objects together to describe knowledge. The potential for the recent announcement of the Google Wave protocol to have a significant impact on realizing this vision is discussed along with the issues of security and provenance that are raised by such an approach.{\textless}/p{\textgreater}
},
author = {{Cameron Neylon}},
doi = {10.1186/1759-4499-1-3},
issn = {1759-4499},
journal = {Autom Exp},
language = {eng},
pages = {3},
title = {{Head in the clouds: Re-imagining the experimental laboratory record for the web-based networked world.}},
volume = {1},
year = {2009}
}
@article{CameronNeylon2011,
abstract = {{\textless}p{\textgreater}In this piece I would like to tell a few stories; three stories to be precise. Firstly I want to explain where I am, where I{\&}{\#}39;ve come from and what has led me to the views that I hold today. I find myself at an interesting point in my life and career at the same point as the research community is undergoing massive change. The second story is one of what the world might look like at some point in the future. What might we achieve? What might it look like? And what will be possible? Finally I want to ask the question of how we get there from here. What is the unifying idea or movement that actually has the potential to carry us forward in a positive way? At the end of this I{\&}{\#}39;m going to ask you, the reader, to commit to something as part of the process of making that happen.{\textless}/p{\textgreater}
},
