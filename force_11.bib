},
author = {{Clifford Tatum} and {Paul Wouters}},
booktitle = {Beyond the PDF2},
keywords = {Visions for the Future},
language = {eng},
title = {{The ACUMEN Portfolio - Accounting for alternative forms of scholarly output}},
url = {http://www.force11.org/Visions},
year = {2013}
}
