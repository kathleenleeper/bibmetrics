year = {2013}
}
@misc{AndrewGrimes2013,
abstract = {{\textless}p{\textgreater}A powerful side-effect of the emerging ORCID standard is the potential uses of ORCID as social sign-in mechanism. Creative uses of the ORCID API (developed by Semantico) will enable the owners of scholarly content platforms to quickly and easily build author-focused services and to provide a shortcut to personalisation without heavy investment in development and infrastructure. Users signing in with an ORCID could be given instant access to research with a fingerprint similar to their own, new papers by co-authors, new content from similar domains, plus a host of other services that would build and promote loyalty to the platform.{\textless}/p{\textgreater}
},
